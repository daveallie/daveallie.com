{
  "singleQuote": true,
  plugins: ['@ianvs/prettier-plugin-sort-imports'],
  "importOrder": [
    "^react$",
    "^[^.~/]",
    "^~((?!(styles\\.module|styles/global)).)*$",
    "^[./]((?!(styles\\.module|styles/global)).)*$",
    "^~((?!styles/global).)*$",
    "^[./]((?!styles/global).)*$",
    "^~",
    "^[./]"
  ]
}
